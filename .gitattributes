# Unreal Engine file types.
[attr]lock filter=lfs diff=lfs merge=lfs -text binary -delta
[attr]lfs filter=lfs diff=lfs merge=lfs -text
*.uasset lock
*.umap lock
*.xd lock
*.wav lfs
*.phononscene lfs
*.probebox lfs
*.probebatch lfs
*.bakedsources lfs
*.dll lfs
*.pdb lfs
*.exe lfs
*.pak lfs
*.bin lfs

# Anything in `/RawContent` dir.
/SourceArt/**/* filter=lfs diff=lfs merge=lfs -text